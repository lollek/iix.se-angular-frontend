<h1>Books</h1>

<%= form_for :book, url: books_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :author %><br>
    <%= f.text_field :author %>
  </p>

  <p>
    <%= f.label :other %><br>
    <%= f.text_field :other %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
